# Tipalti API Configuration
# Copy this file to .env and fill in your credentials

# Your Tipalti payer name (case sensitive)
TIPALTI_PAYER_NAME=your_payer_name

# Your Tipalti master key (from API settings)
TIPALTI_MASTER_KEY=your_master_key_from_dashboard

# Environment: true for sandbox, false for production
TIPALTI_SANDBOX=true
